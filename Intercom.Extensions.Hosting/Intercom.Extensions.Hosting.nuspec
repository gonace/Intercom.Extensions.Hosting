<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Intercom.Extensions.Hosting</id>
    <title>Intercom.Extensions.Hosting</title>
    <version>$version$</version>
    <authors>gonace,laget.se</authors>
    <owners>laget.se</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Hosting and startup abstractions for Intercom. When using NuGet 3.x this package requires at least version 3.4...</description>
    <summary></summary>
    <license type="expression">Apache-2.0</license>
    <projectUrl>https://github.com/gonace/Intercom.Extensions.Hosting</projectUrl>
    <icon>assets\icon.png</icon>
    <readme>assets\README.md</readme>
    <repository type="git" url="https://github.com/gonace/Intercom.Extensions.Hosting" branch="master" />
    <dependencies>
      <group targetFramework="net7.0">
        <dependency id="Intercom-NET" version="0.1.8-alpha" />
        <dependency id="Microsoft.Extensions.Hosting.Abstractions" version="7.0.0" />
      </group>
      <group targetFramework="net6.0">
        <dependency id="Intercom-NET" version="0.1.8-alpha" />
        <dependency id="Microsoft.Extensions.Hosting.Abstractions" version="7.0.0" />
      </group>
        <group targetFramework="netstandard2.1">
            <dependency id="Intercom-NET" version="0.1.8-alpha" />
            <dependency id="Microsoft.Extensions.Hosting.Abstractions" version="7.0.0" />
        </group>
      <group targetFramework="netstandard2.0">
        <dependency id="Intercom-NET" version="0.1.8-alpha" />
        <dependency id="Microsoft.Extensions.Hosting.Abstractions" version="7.0.0" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="..\README.md" target="assets\" />
    <file src="..\assets\icon.png" target="assets\" />
    <file src=".\bin\Release\net7.0\*" target="lib\net7.0\" />
    <file src=".\bin\Release\net6.0\*" target="lib\net6.0\" />
    <file src=".\bin\Release\netstandard2.1\*" target="lib\netstandard2.1\" />
    <file src=".\bin\Release\netstandard2.0\*" target="lib\netstandard2.0\" />
  </files>
</package>
